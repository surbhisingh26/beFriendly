<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
   <filter>
	<filter-name>Call</filter-name>
	<filter-class>com.social.frenz4.actions.Filters</filter-class>
	</filter>
  
  <filter-mapping>
		<filter-name>Call</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
  <servlet>
  	<servlet-name>UserActions</servlet-name>
  	<display-name>UserActions</display-name>
  	<description></description>
  	<servlet-class>com.social.frenz4.actions.UserActions</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FriendActions</servlet-name>
  	<display-name>FriendActions</display-name>
  	<description></description>
  	<servlet-class>com.social.frenz4.actions.FriendActions</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdminActions</servlet-name>
  	<display-name>AdminActions</display-name>
  	<description></description>
  	<servlet-class>com.social.frenz4.actions.AdminActions</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserActions</servlet-name>
  	<url-pattern>/userpaths/*</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>FriendActions</servlet-name>
  	<url-pattern>/friendpaths/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdminActions</servlet-name>
  	<url-pattern>/adminpaths/*</url-pattern>
  </servlet-mapping>
</web-app>
